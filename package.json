{
  "name": "dinero-market",
  "version": "1.0.0",
  "description": "Dinero lending contracts for Interest Protocol",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --testfiles './test/**.ts'",
    "clean": "npx hardhat clean && rimraf cache && rimraf artifacts",
    "compile": "npx hardhat compile",
    "publish": "npm publish contracts --access public",
    "test:nm": "npx hardhat test ./test/native-token-market.test.ts",
    "test:lpm": "npx hardhat test ./test/lp-free-market.test.ts"
  },
  "keywords": [
    "DeFi",
    "Ethereum",
    "Dinero",
    "Crypto"
  ],
  "author": "Interest Labs Inc",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/hardhat-upgrades": "^1.20.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "ethereumjs-util": "^7.1.5",
    "hardhat": "^2.10.1",
    "husky": "^7.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.2",
    "@interest-protocol/dex": "^1.4.0",
    "@interest-protocol/earn": "^1.5.0",
    "@interest-protocol/library": "^1.2.3",
    "@interest-protocol/tokens": "^1.3.0",
    "@openzeppelin/contracts-upgradeable": "^4.7.1"
  }
}
